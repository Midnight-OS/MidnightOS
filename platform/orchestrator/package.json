{
  "name": "@midnightos/orchestrator",
  "version": "1.0.0",
  "description": "Container orchestration for multi-user MidnightOS platform",
  "main": "dist/index.js",
  "scripts": {
    "dev": "PORT=3002 pnpm tsx --watch src/api.ts",
    "build": "tsc",
    "build:contracts": "cd contracts && npm run build",
    "build:cli": "cd cli && npm run build",
    "build:all": "npm run build:contracts && npm run build:cli && npm run build",
    "start": "node dist/api.js",
    "testnet-remote": "cd cli && pnpm tsx src/testnet-remote.ts",
    "test": "pnpm jest",
    "test:watch": "pnpm jest --watch",
    "queries": "pnpm tsx src/queries.ts",
    "test:coverage": "pnpm jest --coverage",
    "test:unit": "pnpm jest tests/*.test.ts",
    "test:integration": "pnpm jest tests/integration/*.test.ts --runInBand",
    "test:ci": "pnpm jest --ci --coverage --maxWorkers=2",
    "db:push": "pnpm prisma db push",
    "db:migrate": "pnpm prisma migrate deploy",
    "db:studio": "pnpm prisma studio"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@prisma/client": "6.16.2",
    "@prisma/extension-optimize": "^2.0.0",
    "@prisma/instrumentation": "^6",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dockerode": "^4.0.2",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^22.0.0",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^9.0.7",
    "bcryptjs": "^3.0.2",
    "fs-extra": "^11.3.2",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prisma": "6.16.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.4",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
